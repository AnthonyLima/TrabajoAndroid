<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".MainActivity">

    <TextView
        android:text="@string/hello_world"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textSaludo" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Saludo"
        android:id="@+id/button"
        android:layout_toEndOf="@+id/textSaludo"
        android:layout_below="@+id/textSaludo"
        android:layout_toRightOf="@+id/textSaludo"
        android:onClick="ClickSaludar" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1"
        android:id="@+id/button2"
        android:layout_toEndOf="@+id/textSaludo"
        android:layout_toStartOf="@+id/button"
        android:layout_below="@+id/button"
        android:layout_toLeftOf="@+id/button"
        android:layout_marginTop="54dp"
        android:onClick="ClickBoton1" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2"
        android:id="@+id/button3"
        android:layout_toEndOf="@+id/button"
        android:layout_below="@+id/button2"
        android:layout_centerHorizontal="true"
        android:onClick="ClickBoton2" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="3"
        android:id="@+id/button4"
        android:layout_toEndOf="@+id/button3"
        android:layout_below="@+id/button3"
        android:layout_centerHorizontal="true"
        android:onClick="ClickBoton3" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="4"
        android:id="@+id/button5"
        android:layout_alignStart="@+id/button2"
        android:layout_below="@+id/button4"
        android:layout_centerHorizontal="true"
        android:onClick="ClickBoton4" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="5"
        android:id="@+id/button6"
        android:layout_toEndOf="@+id/button"
        android:layout_below="@+id/button5"
        android:layout_centerHorizontal="true"
        android:onClick="ClickBoton5" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="6"
        android:id="@+id/button7"
        android:layout_alignStart="@+id/button4"
        android:layout_below="@+id/button6"
        android:layout_centerHorizontal="true"
        android:onClick="ClickBoton6" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="7"
        android:id="@+id/button8"
        android:layout_below="@+id/button5"
        android:layout_toRightOf="@+id/textSaludo"
        android:layout_toEndOf="@+id/textSaludo"
        android:layout_marginTop="54dp"
        android:onClick="ClickBoton7" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="8"
        android:id="@+id/button9"
        android:layout_toEndOf="@+id/button"
        android:layout_below="@+id/button8"
        android:layout_centerHorizontal="true"
        android:onClick="ClickBoton8" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="9"
        android:id="@+id/button10"
        android:layout_alignStart="@+id/button7"
        android:layout_below="@+id/button9"
        android:layout_centerHorizontal="true"
        android:onClick="ClickBoton9" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:id="@+id/button11"
        android:layout_toEndOf="@+id/button8"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/button8"
        android:layout_marginBottom="30dp"
        android:onClick="ClickBoton0" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="phone"
        android:ems="10"
        android:id="@+id/editText"
        android:layout_below="@+id/button"
        android:layout_alignRight="@+id/button11"
        android:layout_alignEnd="@+id/button11" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="phone"
        android:ems="10"
        android:id="@+id/editText2"
        android:layout_alignBottom="@+id/button"
        android:layout_alignRight="@+id/button4"
        android:layout_alignEnd="@+id/button4" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="phone"
        android:ems="10"
        android:id="@+id/editText3"
        android:layout_below="@+id/button"
        android:layout_alignRight="@+id/button4"
        android:layout_alignEnd="@+id/button4" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/editText4"
        android:layout_above="@+id/button2"
        android:layout_alignRight="@+id/button4"
        android:layout_alignEnd="@+id/button4" />

</RelativeLayout>
